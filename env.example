# 데이터베이스 설정 (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=your_password_here

# 네이버 OCR API 설정 (선택사항 - 없으면 더미 데이터 사용)
NAVER_OCR_API_URL=https://your-ocr-api-url.apigw.ntruss.com/custom/v1/your-domain/your-ocr-api
NAVER_OCR_SECRET_KEY=your_naver_ocr_secret_key_here

# 애플리케이션 설정
DEBUG=True
APP_ENV=development

# 사용법:
# 1. 이 파일을 .env로 복사하세요: cp env.example .env
# 2. 실제 값들로 수정하세요
# 3. OCR API가 없어도 더미 데이터로 테스트 가능합니다 